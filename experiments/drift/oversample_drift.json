{
  "dataset_class": "iceberg.iceberg_dataset.IcebergDataSet",
  "dataset": "../output/merged_dataset.json",
  "output": "../output/oversampling_drift.json",
  "bins":
    [
      ["no_iceberg", 0],
      ["iceberg", 1]
    ],
  "drift_scenario":
  {
    "condition": "oversampling",
    "module": "prevalence_drift",
    "params":
    {
      "max_num_samples":  12000,
      "timebox_size": 1000,
      "prevalence_bin": 1,
      "prevalences": [65, 63, 60, 55, 45, 40],
      "prevalence_repeat": true
    }
  }
}

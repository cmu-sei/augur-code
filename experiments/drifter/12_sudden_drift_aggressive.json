{
  "dataset_class": "iceberg.iceberg_dataset.IcebergDataSet",
  "dataset": "../output/merged_dataset.json",
  "output": "../output/12_sudden_drift_aggressive.json",
  "bins":
    [
      ["no_iceberg", 0],
      ["iceberg", 1]
    ],
  "timestamps":
  {
    "start_datetime": "2023-03-18",
    "increment_unit": "H"
  },
  "drift_scenario":
  {
    "condition": "sudden aggressive",
    "module": "prevalence_drift",
    "params":
    {
      "max_num_samples": 9999,
      "sample_group_size": 3333,
      "prevalences":
      {
        "1":
        {
          "percentages_by_sample_group": [65, 10, 70],
          "prevalence_repeat": false
        }
      }
    }
  }
}

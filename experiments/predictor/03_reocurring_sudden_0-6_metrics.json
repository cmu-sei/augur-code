{
  "mode": "predict",
  "threshold": 0.5,
  "input":
  {
    "dataset_class": "iceberg.iceberg_dataset.IcebergDataSet",
    "dataset": "../output/03_reocurring_sudden_drift.json",
    "base_dataset": "../output/merged_dataset.json",
    "model": "../output/trained_model",
    "ts_model": "../output/trained_model_ts"
  },
  "output":
  {
    "predictions_output": "../output/03_reocurring_sudden_0-6_predictions.json",
    "metrics_output": "../output/03_reocurring_sudden_0-6_metrics.json"
  },
  "time_interval":
  {
    "starting_interval": "2023-03-18",
    "interval_unit": "D"
  },
  "metrics":
  [
    {
      "name": "Kullback-Leibler Divergence",
      "type": "DistanceMetric",
      "module": "kl_divergence",
      "params":
      {
        "distribution": "kernel_density",
        "range_start": -1,
        "range_end": 1000,
        "range_step": 1
      }
    },
    {
      "name": "Hellinger Distance",
      "type": "DistanceMetric",
      "module": "hellinger",
      "params":
      {
        "distribution": "kernel_density",
        "range_start": -1,
        "range_end": 1000,
        "range_step": 1
      }
    },
    {
      "name": "Total Variation Distance",
      "type": "DistanceMetric",
      "module": "tvd",
      "params":
      {
        "distribution": "kernel_density",
        "range_start": -1,
        "range_end": 1000,
        "range_step": 1
      }
    },
    {
      "name": "Kolmogorov-Smirnov Statistic",
      "type": "ErrorMetric",
      "module": "kolmogorov_smirnov"
    },
    {
      "name": "Wasserstein Distance",
      "type": "DistanceMetric",
      "module": "wasserstein",
      "params":
      {
        "distribution": "kernel_density",
        "range_start": -1,
        "range_end": 1000,
        "range_step": 1
      }
    },
    {
      "name": "Energy Distance",
      "type": "DistanceMetric",
      "module": "energy",
      "params":
      {
        "distribution": "kernel_density",
        "range_start": -1,
        "range_end": 1000,
        "range_step": 1
      }
    },
    {
      "name": "Z Test / Student's t",
      "type": "ErrorMetric",
      "module": "z_test"
    }
  ]
}


{
  "dataset_class": "iceberg.iceberg_dataset.IcebergDataSet",
  "dataset": "../output/merged_dataset.json",
  "output": "../output/drift.json",
  "bins":
    [
      ["no_iceberg", 0],
      ["iceberg", 1]
    ],
  "timestamps":
  {
    "enabled": true,
    "start_datetime": "2020-04-01",
    "increment": 1
  },
  "drift_scenario":
  {
    "condition": "random",
    "module": "random_drift",
    "params":
    {
      "max_num_samples":  20,
      "timebox_size": 5
    }
  }
}
